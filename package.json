{
	"name": "deprecations",
	"version": "3.0.3",
	"author": "Jordan Harband <ljharb@gmail.com>",
	"funding": {
		"url": "https://github.com/sponsors/ljharb"
	},
	"description": "Given npm module names, get a JSON map of npm version numbers to deprecation messages.",
	"license": "MIT",
	"main": "index.js",
	"exports": {
		".": "./index.js",
		"package.json": "./package.json"
	},
	"types": "./deprecations.d.ts",
	"sideEffects": false,
	"scripts": {
		"prepack": "npmignore --auto --commentLines=autogenerated",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"prepublishOnly": "safe-publish-latest",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\"",
		"pretest": "npm run lint",
		"test": "npm run tests-only",
		"tests-only": "nyc tape 'test/**/*.js'",
		"posttest": "aud --production",
		"lint": "eslint --ext=js,mjs ."
	},
	"preferGlobal": true,
	"bin": {
		"deprecations": "./bin.js"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ljharb/npm-deprecations.git"
	},
	"keywords": [
		"deprecations",
		"npm",
		"deprecated",
		"deprecation messages"
	],
	"dependencies": {
		"array.prototype.every": "^1.1.5",
		"object.entries": "^1.1.7",
		"object.fromentries": "^2.0.7",
		"pacote": "^17.0.6",
		"string.prototype.trim": "^1.2.8"
	},
	"devDependencies": {
		"@ljharb/eslint-config": "^21.1.0",
		"@types/array.prototype.every": "^1.1.0",
		"@types/object.entries": "^1.1.0",
		"@types/object.fromentries": "^2.0.4",
		"@types/pacote": "^11.1.8",
		"@types/tape": "^5.6.4",
		"aud": "^2.0.4",
		"auto-changelog": "^2.4.0",
		"eslint": "=8.8.0",
		"in-publish": "^2.0.1",
		"npmignore": "^0.3.1",
		"nyc": "^15.1.0",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.7.4",
		"typescript": "^5.4.0-dev.20240127"
	},
	"engines": {
		"node": "^18.17.0 || >=20.5.0"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true,
		"startingVersion": "4.0.0"
	},
	"publishConfig": {
		"ignore": [
			".github/workflows"
		]
	}
}
